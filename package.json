{
  "name": "backend",
  "version": "1.0.0",
  "description": "Desktop app for boss sync API (cross-platform)",
  "main": "electron/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "set NODE_ENV=development && nodemon src/server.js",
    "module": "node dev/scripts/moduleCreator.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "dev:electron": "concurrently \"npm run dev\" \"electron .\"",
    "seed-db": "node dev/scripts/seeders/seed_migrateDB.js",
    "seed-clear": "node dev/scripts/seeders/seed_migrateDB.js --clear",
    "seed:all": "node dev/scripts/seeders/seed_migrateDB.js --generate-users --seed-faces",
    "seed:full": "node dev/scripts/seeders/seed_migrateDB.js --clear --generate-users --seed-faces",
    "build:electron": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "pack:electron": "electron-builder --dir",
    "dev2": "concurrently \"nodemon src/server.js\" \"electron .\"",
    "electron": "electron .",
    "announcement": "node dev/scripts/generateAnnouncement.js",
    "announcement:am": "node dev/scripts/generateAnnouncement.js --locale am",
    "announcement:or": "node dev/scripts/generateAnnouncement.js --locale or",
    "announcement:clear": "node dev/scripts/generateAnnouncement.js --clear",
    "announcement:clear:am": "node dev/scripts/generateAnnouncement.js --clear --locale am",
    "download-models": "node scripts/download-models.js",
    "setup": "npm run download-models && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@google/generative-ai": "^0.24.1",
    "@huggingface/inference": "^4.13.4",
    "@imgly/background-removal-node": "^1.4.5",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@prisma/client": "^6.16.1",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "canvas": "^3.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "ffmpeg-static": "^5.3.0",
    "free-background-remover": "^1.0.6",
    "fs-extra": "^11.3.3",
    "google-auth-library": "^10.5.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "i": "^0.3.7",
    "i18next": "^23.16.8",
    "i18next-fs-backend": "^2.6.1",
    "i18next-http-middleware": "^3.9.2",
    "i18next-icu": "^2.4.1",
    "jimp": "^1.6.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.5",
    "nanoid": "^5.1.6",
    "next": "^16.1.4",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^7.0.6",
    "nodemon": "^3.1.11",
    "npm": "^11.8.0",
    "onnxruntime-node": "^1.23.2",
    "openai": "^5.23.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^2.16.105",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "tesseract.js": "^7.0.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "concurrently": "^7.6.0",
    "prisma": "^6.18.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
